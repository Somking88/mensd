$(function(){
  $(document).on("click", ".js-restore-form", function (e) {
    e.preventDefault();
    var $obj = $(this);
    $obj.closest(".js-loader-wrapper").find(".js-success").hide();
    $obj.closest(".js-loader-wrapper").find(".js-fields").show();

  })


})

$(function () {
  console.log("asdadwawdawdawda");
  $(".js-datepicker").datepicker({
  });
  $(".js-send-order-bills").on("click", function (e) {
    e.preventDefault();

    var $link = $(this);
    var $wrapper = $(this).closest(".order-table__td");
    $wrapper.addClass("_loading");
    $.ajax({
      url: $link.data("path"),
      type: "get",
      data: {
        "order_id": $link.data("order-id")
      },
      dataType: 'json',

      complete() {
        $wrapper.removeClass("_loading");
      },
      success(response) {
        $wrapper.removeClass("_loading");
        if (response.status) {
          Toastify({
            text: "Документы сгенерированы и отправлены на вашу почту",
            duration: 1500,
            newWindow: true,
            close: true,
            gravity: "top", // `top` or `bottom`
            backgroundColor: "linear-gradient(to right, #00b09b, #96c93d)",
            stopOnFocus: true, // Prevents dismissing of toast on hover
            onClick: function () {
            } // Callback after click
          }).showToast();

        } else {
          if (typeof response.globalMessage != "undefined") {
            Toastify({
              text: response.globalMessage,
              duration: 1500,
              newWindow: true,
              close: true,
              gravity: "top", // `top` or `bottom`
              backgroundColor: "linear-gradient(to right, #f44336, #b0281f)",
              stopOnFocus: true, // Prevents dismissing of toast on hover
              onClick: function () {
              } // Callback after click
            }).showToast();
          }
          if (typeof response.errors != "undefined") {
            var strErrors = '';
            for (let i in response.errors) {
              strErrors += response.errors[i]
            }

            Toastify({
              text: strErrors,
              duration: 1500,
              newWindow: true,
              close: true,
              gravity: "top", // `top` or `bottom`
              backgroundColor: "linear-gradient(to right, #f44336, #b0281f)",
              stopOnFocus: true, // Prevents dismissing of toast on hover
              onClick: function () {
              } // Callback after click
            }).showToast();
          }
        }
      }
    });


  })

  $(".js-locate-me").on("click", function (e) {
    e.preventDefault();
    if ("geolocation" in navigator) {
      /* геолокация доступна */
      navigator.geolocation.getCurrentPosition(function (position) {
          $.ajax({
            url: 'https://maps.googleapis.com/maps/api/geocode/json?latlng=' +
              '' + position.coords.latitude + ',' + position.coords.longitude + '&key=AIzaSyDDmeco-YPbWAuM8Ga7Kd5VdtSldfgsxHc',
            success: function (response) {
              var results = response.results
              var city = parseCity(results)
              var isFindCity = false;

              for (var i in window.cities) {
                var cityItem = window.cities[i];
                if (city == cityItem.name) {
                  isFindCity = true;


                  Toastify({
                    text: "Ваш город: " + city + ". <br /> Сейчас вы будете перенаправлены",
                    duration: 1500,
                    newWindow: true,
                    close: true,
                    gravity: "top", // `top` or `bottom`
                    backgroundColor: "linear-gradient(to right, #00b09b, #96c93d)",
                    stopOnFocus: true, // Prevents dismissing of toast on hover
                    onClick: function () {
                    } // Callback after click
                  }).showToast();
                  setTimeout(function () {
                    window.location = cityItem.link;
                  }, 1200)
                  break;
                }
              }

              if (!isFindCity) {
                Toastify({
                  text: "Ваш город: " + city + ".<br /> К сожалению, в этом городе мы не работаем.",
                  duration: 1500,
                  newWindow: true,
                  close: true,
                  gravity: "top", // `top` or `bottom`
                  backgroundColor: "linear-gradient(to right, #f44336, #b0281f)",
                  stopOnFocus: true, // Prevents dismissing of toast on hover
                  onClick: function () {
                  } // Callback after click
                }).showToast();
              }

            }
          })
        },
        function () {
          Toastify({
            text: "Для определения города необходимо дать разрешение в браузере",
            duration: 1500,
            newWindow: true,
            close: true,
            gravity: "top", // `top` or `bottom`
            backgroundColor: "linear-gradient(to right, #f44336, #b0281f)",
            stopOnFocus: true, // Prevents dismissing of toast on hover
            onClick: function () {
            } // Callback after click
          }).showToast();
        });
    }

  })

  $(".js-ajax-form").each(function () {
    initAjaxForm($(this))
  })

  $(document).on("click", ".js-counter-minus", function (e) {
    var $counter = $(this).closest(".js-counter")
    var curValue = parseInt($counter.find(".js-counter-value").text())
    if (curValue < 11) curValue = 11

    setCounterValue($counter, (curValue - 1))
  })
  $(document).on("click", ".js-counter-plus", function (e) {
    var $counter = $(this).closest(".js-counter")
    var curValue = parseInt($counter.find(".js-counter-value").text())

    setCounterValue($counter, (curValue + 1))
  })

  $(document).on("click", ".js-recommend-review-btn", function (e) {
    e.preventDefault();
    var $btn = $(this);
    var $item = $btn.closest(".js-recommend-review");
    var type = "increase";

    if ($btn.hasClass("negative")) {
      type = "decrease";
    }

    $.ajax({
      url: $item.data("path"),
      type: 'GET',
      dataType: 'json',
      data: {
        type: type,
        id: $item.data("review-id")
      },
      success: function (data) {
        if (data.status) {
          $item.find(".js-recommend-review-btn.positive").find("span").text(data.ok_count);
          $item.find(".js-recommend-review-btn.negative").find("span").text(data.fail_count);

          var voteReviews = Cookies.get('vote_reviews')
          if (typeof voteReviews == "undefined") {
            voteReviews = {}
          } else {
            voteReviews = JSON.parse(voteReviews)
          }

          if (type == 'increase') {
            voteReviews[$item.data("review-id")] = true;
            $item.find(".js-recommend-review-btn.positive").addClass("_active");
            $item.find(".js-recommend-review-btn.negative").removeClass("_active");
          } else {
            voteReviews[$item.data("review-id")] = false;
            $item.find(".js-recommend-review-btn.negative").addClass("_active");
            $item.find(".js-recommend-review-btn.positive").removeClass("_active");
          }

          if (data.remove_item_cookie) {
            $item.find(".js-recommend-review-btn").removeClass("_active");
            delete voteReviews[$item.data("review-id")];
          }

          Cookies.set('vote_reviews', JSON.stringify(voteReviews), {expires: 90});

          Toastify({
            text: $item.data("ok-message"),
            duration: 1500,
            newWindow: true,
            close: true,
            gravity: "top", // `top` or `bottom`
            backgroundColor: "linear-gradient(to right, #00b09b, #96c93d)",
            stopOnFocus: true, // Prevents dismissing of toast on hover
            onClick: function () {
            } // Callback after click
          }).showToast();
        }
      },
      complete: function () {
        $btn.removeClass("_loading")
      }
    });


  })

  $(".js-code-field").mask('0000', {
    placeholder: "XXXX"
  });

  initPhoneInput()

  $(window).scroll(function () {
    if ($(this).scrollTop() > 200) {
      $('.js-scroll-up').fadeIn(400);
    } else {
      $('.js-scroll-up').fadeOut(400);
    }
  });

  $('.js-scroll-up').click(function () {
    $('body,html').animate({scrollTop: 0}, 800);
  });

  $(document).on("click", "[data-gtm-click]", function () {
    dataLayer.push({'event': $(this).data("gtm-click")});
  })

  $(document).on("click", ".hoverl_6R", function () {
    dataLayer.push({'event': 'chat'});
  })


  $(window).on('load', function () {
    var lazyLoadInstance = new LazyLoad({
      elements_selector: ".lazy",
    });
  })
  window.onpopstate = function (event) {
    if (!event.state) return;

    if (event.state.is_ajax_request) {
      loadUrl(document.location, false, function () {
      }, false);
    } else {
      window.location = document.location;
    }
  };

  $(document).on("click", ".js-ajax-href", function (e) {
    e.preventDefault();
    var $href = $(this).attr("href");
    if (!$href || $href == 'javascript:void(0)') {
      $href = $(this).data("href");
    }

    var skipLocation = false;
    if ($(this).hasClass("_skip-location")) {
      skipLocation = true;
    }

    loadUrl($href, skipLocation, function () {

    }, false);
  });

  $(document).on("change", ".js-filter-select", function (e) {
    e.preventDefault();
    var $href = $(this).data("href");

    var skipLocation = false;
    if ($(this).hasClass("_skip-location")) {
      skipLocation = true;
    }

    if (!$(this).hasClass("disabled")) {
      loadUrl($href, skipLocation, function () {


      }, "#mobile-filter");
    }

  });

  $(document).on("click", ".js-loader-list-button", function (e) {
    e.preventDefault();
    var $link = $(this);
    if (!$link.hasClass("_loading")) {
      var $loadingContent = $link.closest(".js-loader-list-content");
      var href = $link.data("href");

      $link.addClass("_loading");
      $loadingContent.load(href + "&is_ajax=1", function () {
        var lazyLoadInstance = new LazyLoad({
          elements_selector: ".lazy",
        });
        /* if (typeof window.history != 'undefined') {
             window.history.pushState({}, '', href);
         }*/
      });
    }
  });


  $(document).on("click", ".js-add-cart", function (e) {
    e.preventDefault();
    var $btn = $(this);
    var additionals = $btn.data("additionals")
    var gifts = $btn.data("gifts")
    var productId = $btn.data("product-id")
    var path = $btn.data("add-path")
    var lang = $btn.data("lang")
    var data = {
      productId: productId,
      gifts: gifts,
      additionals: additionals,
      lang: lang
    };

    if ($btn.data("is-constructor") == '1') {
      var $product = $btn.closest(".js-product-card")
      data.is_constructor = true;

      $product.find(".js-constructor").find(".js-current-value").each(function () {
        var $val = $(this);
        data[$val.data("key")] = $val.data("value");
      })
    }

    if ($btn.hasClass("_is-complect")) {
      data["is_complect"] = true;
      data["complect"] = $btn.data("complect");
    }

    $btn.addClass("_loading")

    $.ajax({
      url: path,
      type: 'GET',
      dataType: 'json',
      data: data,
      success: function (data) {
        if (data.status) {
          $(".js-basket-count").text(data.count);
          $(".js-basket-informer-text").html(data.text);
          $btn.addClass("btn_secondary");
          $btn.text($btn.data("added-text"));
          $btn.removeClass("js-add-cart");
          $btn.attr("onclick", 'window.location.href="' + $btn.data("basket-page") + '"');
          if ($btn.hasClass("force-to-basket")) {
            window.location.href = $btn.data("basket-page");
          }
        }
      },
      complete: function () {
        $btn.removeClass("_loading")
      }
    });
  })


  if ($(".js-product-card").length) {
    $(document).on("click", ".js-gift-item-add", function (e) {
      var $gift = $(this).closest(".js-gift-item");

      $gift.addClass("_added")

      makeGiftsArray($gift.closest(".js-product-card"))
    })

    $(document).on("click", ".js-gift-item-plus", function (e) {
      var $gift = $(this).closest(".js-gift-item");

      var count = parseInt($gift.find(".js-gift-item-count").text());
      $gift.find(".js-gift-item-count").text(count + 1);

      makeGiftsArray($gift.closest(".js-product-card"))
    })

    $(document).on("click", ".js-gift-item-minus", function (e) {
      var $gift = $(this).closest(".js-gift-item");

      var count = parseInt($gift.find(".js-gift-item-count").text());
      if (count == 1) {
        $gift.removeClass("_added")
      } else {
        $gift.find(".js-gift-item-count").text(count - 1);
      }

      makeGiftsArray($gift.closest(".js-product-card"))
    })

    $(document).on("click", ".js-gift-item-remove", function (e) {
      var $gift = $(this).closest(".js-gift-item");

      $gift.removeClass("_added")

      makeGiftsArray($gift.closest(".js-product-card"))
    })
  }

  $(document).on("change", ".js-payment-input", function (e) {
    var baseCurrency = $("body").data("currency")
    var paymentCode = $(this).data("code")

    if (paymentCode == 'paypal' && baseCurrency == 'kzt') {
      $(".js-order-currency").val("usd")
    } else if (paymentCode == 'kazkom' && baseCurrency == 'rub') {
      $(".js-order-currency").val("kzt")
    } else if (paymentCode == "cloud_payments") {
      $(".js-order-currency").val("kzt")
    } else {
      $(".js-order-currency").val(baseCurrency)
    }
  })

  $(document).on("click", ".js-filter-more", function (e) {
    e.stopPropagation();
    e.preventDefault();
    $(this).closest(".js-filter-field").find(".hidden-filter-item").show();
    $(this).hide()
  })
})

function makeGiftsArray($productItem) {
  var gifts = []
  var totalPrice = 0
  var totalOldPrice = 0

  $productItem.find(".js-gift-item._added").each(function () {
    var $item = $(this)
    totalPrice += parseInt($item.data("price"))
    totalOldPrice += parseInt($item.data("price"))
    gifts.push({
      "id": $item.data("id"),
      "quantity": 1
    })

  })

  $productItem.find(".js-add-cart").data("gifts", gifts)

  var currency = $productItem.find(".js-price").find(".curs").text()

  var $price = $productItem.find(".js-price")
  var $oldPrice = $productItem.find(".js-old-price")
  var newPrice = parseInt($price.data("price")) + totalPrice
  var newOldPrice = parseInt($oldPrice.data("price")) + totalPrice


  $price.html(newPrice + "<span class='curs'>" + currency + "</span>");
  $oldPrice.html(newOldPrice + "<span class='curs'>" + currency + "</span>");
}

function loadUrl(url, skipLocation, callback, specifyContainer) {
  var selector = "#main-content-wrapper";
  if (specifyContainer) {
    selector = specifyContainer;
  }
  $(selector).addClass("_is-loading");
  $(selector).load(url + " " + selector + ">*", function (responseText) {

    var title = responseText.match(/<title>(.*)<\/title>/);
    title = title[1];
    $("title").html(title);

    if (!skipLocation) {
      history.pushState({'is_ajax_request': true}, '', url);
    }

    $(selector).removeClass("_is-loading");
    window.initProductCard();z
    var lazyLoadInstance = new LazyLoad({
      elements_selector: ".lazy",
    });
    $('.select-flower').select2({
      minimumResultsForSearch: -1,
      theme: 'mobile'
    })

    callback();

  });
}

function initPhoneInput() {

  $('.js-phone-field').each(function () {
    var $obj = $(this);
    var prefix = String($obj.data("prefix"));
    var prefixLength = prefix.length;
    var prefixMask = '';
    for (var i = 1; i <= prefixLength; i++) {
      prefixMask += '0';
    }
    var defaultMask = '+' + prefixMask + ' (000) 000-00000';

    $obj.mask(defaultMask, {
      placeholder: "+" + prefix + " (ххх) ххх-хххх",
      onKeyPress: function (cep, e, field, options) {
        var value = String($obj.cleanVal(cep));
        if (value.length >= 10) {
          var newMask = ' (000) 000-00000';
          var delta = value.length - 10;
          if (delta < 1) delta = 1;

          var prefixMask = '';

          for (var i = 1; i <= delta; i++) {
            prefixMask += '0';
          }
          newMask = '+' + prefixMask + newMask;
          $obj.mask(newMask, options);
        } else {
          $obj.mask(defaultMask, options);
        }
        /*var masks = ['00000-000', '0-00-00-00'];
        var mask = (cep.length > 7) ? masks[1] : masks[0];
        $('.crazy_cep').mask(mask, options);*/
      }
    });

  });
}


function setCounterValue($counter, newValue) {
  $counter.find(".js-counter-value").text(newValue);
  $(".js-quantity-btn").removeClass("btn_secondary_active")

  if ($(".js-quantity-" + newValue).length) {
    $(".js-quantity-" + newValue).addClass("btn_secondary_active")
    $counter.find(".product-card-add__item-add-cell_value").removeClass("btn_secondary_active")
  } else {
    $counter.find(".product-card-add__item-add-cell_value").addClass("btn_secondary_active")
  }


  clearTimeout(window.counterTimeout)
  window.counterTimeout = setTimeout(function () {
    var href = $(".js-counter-new-link").data("href-ref")

    href = href.replace("#Q_CODE#", newValue)
    $(".js-counter-new-link").attr("href", href)
    $(".js-counter-new-link").data("href", href)
    $(".js-counter-new-link").attr("data-href", href)
    $(".js-counter-new-link").trigger("click")


  }, 1500);

}

function parseCity(results) {
  //return 'Актобе';
  if (results[1]) {
    var country = null, countryCode = null, city = null, cityAlt = null;
    var c, lc, component;
    for (var r = 0, rl = results.length; r < rl; r += 1) {
      var result = results[r];

      if (!city && result.types[0] === 'locality') {
        for (c = 0, lc = result.address_components.length; c < lc; c += 1) {
          component = result.address_components[c];

          if (component.types[0] === 'locality') {
            city = component.long_name;
            break;
          }
        }
      } else if (!city && !cityAlt && result.types[0] === 'administrative_area_level_1') {
        for (c = 0, lc = result.address_components.length; c < lc; c += 1) {
          component = result.address_components[c];

          if (component.types[0] === 'administrative_area_level_1') {
            cityAlt = component.long_name;
            break;
          }
        }
      } else if (!country && result.types[0] === 'country') {
        country = result.address_components[0].long_name;
        countryCode = result.address_components[0].short_name;
      }

      if (city && country) {
        break;
      }
    }

    if (!city) {
      return cityAlt;
    }

    return city;
  }
}
